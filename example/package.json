{
  "name": "ironalarm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "bun run generate:api-docs && alchemy dev",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "build": "bun run generate:api-docs && vite build",
    "preview": "vite preview",
    "prepare": "bun run generate:api-docs && svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate:api-docs": "bun run scripts/generate-api-docs.ts",
    "db:better-auth": "bunx @better-auth/cli generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:local": "wrangler d1 migrations apply ironalarm-db --local",
    "db:remote": "wrangler d1 migrations apply ironalarm-db --remote"
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "runed": "^0.37.1",
    "svelte": "^5.46.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "@effect/platform-browser": "^0.74.0",
    "alchemy": "^0.82.1",
    "better-auth": "1.4.7",
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.13",
    "hono": "^4.11.1",
    "shiki": "^3.20.0",
    "three": "^0.182.0"
  },
  "type": "module"
}
